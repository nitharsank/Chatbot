{
  "expo": {
    "name": "Chatbot",
    "slug": "Chatbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../Chatbot/app/assets/icon.png",
    "splash": {
      "image": "../Chatbot/app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../Chatbot/app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nith.foodfinder"
    },
    "web": {
      "favicon": "../Chatbot/app/assets/favicon.png"
    }
  }
}
